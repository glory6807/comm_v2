<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="free">  
  <select id="selectList">
    SELECT BOARD_TTL
          ,CRT_DT
          ,IS_DEL
      FROM BOARD_FREE 
     WHERE IS_DEL = 'Y'
  </select>
</mapper>