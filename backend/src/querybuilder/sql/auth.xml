<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="board">  
  <select id="selectSnsUserByEmail">
    SELECT COUNT(1)
      FROM TM_CM_USR
     WHERE EMAIL = #{email}
  </select>

</mapper>